   "source": [
    "# Program to count the number of vowels in a string\n",
    "string = \"Hello, World!\"\n",
    "vowels = \"aeiouAEIOU\"\n",
    "count = sum(1 for char in string if char in vowels)\n",
    "print(f\"String: {string}\")\n",
    "print(f\"Number of vowels: {count}\")"
   ]
  }
