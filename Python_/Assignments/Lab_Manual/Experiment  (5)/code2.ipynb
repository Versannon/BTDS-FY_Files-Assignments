   "source": [
    "Write a Python program to find the intersection and union of two sets."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "57d0d430",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Intersection: {3, 4}\n",
      "Union: {1, 2, 3, 4, 5, 6}\n"
     ]
    }
   ],
   "source": [
    "set1 = {1, 2, 3, 4}\n",
    "set2 = {3, 4, 5, 6}\n",
    "\n",
    "intersection = set1 & set2\n",
    "print('Intersection:', intersection)\n",
    "\n",
    "union = set1 | set2\n",
    "print('Union:', union)"
   ]
  }
