   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "fac = 1\n",
    "if num < 0:\n",
    "    print(\"Factorial does not exist for negative numbers.\")\n",
    "elif num == 0:\n",
    "    print(\"The factorial of 0 is 1.\")\n",
    "else:\n",
    "    for i in range(num, 1, -1):\n",
    "        fac *= i\n",
    "    print(f\"The factorial of {num} is {fac}.\")"
   ]
  }
