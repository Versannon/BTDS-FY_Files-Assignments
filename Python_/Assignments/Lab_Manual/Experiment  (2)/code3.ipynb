   ],
   "source": [
    "def fb_series(n):\n",
    "    a, b = 0, 1\n",
    "    for _ in range(n):\n",
    "        print(a, end=\" \")\n",
    "        a, b = b, a + b\n",
    "\n",
    "# Input from the user\n",
    "n = int(input(\"Enter the number of terms: \"))\n",
    "print(\"Fibonacci Series of \", n, \", is :\")\n",
    "fb_series(n)"
   ]
  }
